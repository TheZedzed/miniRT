FORMAT : .rt

[R]esolution : 
	Identifier		: R
	1st argument 	: screen widht
	2nd argument 	: screen height
	Example			: R 1920 1080

[A]mbient lightning : 
	Identifier		: A
	1st argument	: ambient lighting ratio range [0.0,1.0]
	2nd argument	: RGB colors range [0,255]
	Example			: A 0.42	255,0,255

[C]amera			:
	Identifier		: C
	1st argument	: position of the view point 
	2nd argument	: orientation range [-1,1]
	3rd argument	: Field of view FOV range [0,180]
	Example			: C 42,42,-42	0,0,-1	60

[L]ight				:
	Identifier		: L
	1st argument	: position of the light point
	2nd argument	: light brightness range [0.0,1.0]
	3rd argument	: RGB colors range [0,255]
	Example			: L 42,42,-42	0.42	255,255,255

[sp]here			:
	Identifier		: sp
	1st argument	: position of the sphere
	2nd argument	: sphere's diameter
	3rd argument	: RGB colors range [0,255]
	Example			: sp 42,42,-42	42	255,255,255

[pl]ane				:
	Identfier		: pl
	1st argument	: position of the plane
	2nd argument	: orientation range [-1,1]
	3rd argument	: RGB colors range [0,255]
	Example			: pl 42,42,-42	0,-1,0	255,255,255

[sq]uare			:
	Identifier		: sq
	1st argument	: position of the square's center
	2nd argument	: orientation range [-1,1]
	3rd argument	: sides size
	4th argument	: RGB colors range [0,255]
	Example			: sq 42,42,-42	0,-1,0	42	255,255,255

[cy]linder			:
	Identifier		: cy
	1st argument	: position of the cylinder's
	2nd argument	: orientation range [-1,1]
	3rd argument	: cylinder's diameter
	4th argument	: cylinder's height
	5th argument	: RGB colors range [0,255]
	Example			: cy 42,42,-42	0,-1,0	42	42	255,255,255

[tr]iangle			:
	Identifier		: tr
	1st argument	: position of the first top
	2nd argument	: position of the second top
	3rd argument	: position of the third top
	4th argument	: RGB colors range [0,255]
